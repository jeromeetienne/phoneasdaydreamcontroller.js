{
  "name": "phoneasvrcontroller",
  "version": "0.0.1",
  "description": "using your phone as a controller for VR",
  "dependencies": {
    "socket.io": "^1.5.1"
  },
  "scripts": {
    "start": "node server/server.js",
    "build": "cat src/phone*.js src/three.js/Quaternion.js src/three.js/Euler.js node_modules/socket.io/node_modules/socket.io-client/dist/socket.io.js > build/phoneasvrcontroller.js",
    "minify": "uglifyjs build/phoneasvrcontroller.js > build/phoneasvrcontroller.min.js",
    "watch": "watch 'npm run build' src/"
  }
}
